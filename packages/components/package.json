{
  "name": "@mono/components",
  "version": "1.0.0",
  "private": true,
  "description": "components",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "pnpm run build:scss && tsc",
    "build:scss": "cpx \"src/**/*.scss\" dist",
    "dev": "tsc --watch",
    "lint": "pnpm run lint:js && pnpm run lint:styles",
    "lint:js": "eslint . --fix",
    "lint:styles": "prettier . --write --list-different",
    "test": "vitest run"
  },
  "dependencies": {
    "@mono/data": "workspace:*",
    "@mono/hooks": "workspace:*",
    "@mono/next-js": "workspace:*",
    "@mono/search-site": "workspace:*",
    "@mono/utils": "workspace:*",
    "@react-google-maps/api": "2.20.6",
    "classnames": "2.5.1",
    "datocms-structured-text-utils": "5.0.0",
    "react": "19.1.0",
    "react-datocms": "7.2.2",
    "react-dom": "19.1.0",
    "urql": "4.2.2"
  },
  "devDependencies": {
    "@mono/eslint-config": "workspace:*",
    "@mono/typescript-config": "workspace:*",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@types/node": "24.6.0",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react": "4.6.0",
    "cpx2": "8.0.0",
    "dotenv": "16.5.0",
    "eslint": "9.31.0",
    "eslint-plugin-testing-library": "7.6.6",
    "jsdom": "26.1.0",
    "prettier": "3.6.2",
    "sass": "1.89.2",
    "turbo": "2.5.5",
    "typescript": "5.8.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  }
}
