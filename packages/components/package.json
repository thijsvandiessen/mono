{
  "name": "@mono/components",
  "version": "1.0.0",
  "private": true,
  "description": "components",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "pnpm run build:scss && tsc",
    "build:scss": "cpx \"src/**/*.scss\" dist",
    "dev": "tsc --watch",
    "lint": "pnpm run lint:js && pnpm run lint:styles",
    "lint:js": "eslint",
    "lint:styles": "prettier . --write --list-different",
    "test": "vitest run -c ./vitest.config.ts"
  },
  "dependencies": {
    "@mono/data": "workspace:*",
    "@mono/hooks": "workspace:*",
    "@mono/next-js": "workspace:*",
    "@mono/search-site": "workspace:*",
    "@mono/utils": "workspace:*",
    "@react-google-maps/api": "2.20.8",
    "classnames": "2.5.1",
    "datocms-structured-text-utils": "5.1.7",
    "react": "19.2.3",
    "react-datocms": "7.2.7",
    "react-dom": "19.2.3",
    "urql": "5.0.1"
  },
  "devDependencies": {
    "@mono/eslint-config": "workspace:*",
    "@mono/typescript-config": "workspace:*",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@types/node": "25.0.10",
    "@types/react": "19.2.9",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.2",
    "cpx2": "8.0.0",
    "dotenv": "17.2.3",
    "eslint": "9.39.2",
    "eslint-plugin-testing-library": "7.15.4",
    "jsdom": "27.4.0",
    "prettier": "3.8.1",
    "sass": "1.97.3",
    "turbo": "2.7.5",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "6.0.4",
    "vitest": "4.0.18"
  }
}
