{
  "name": "@mono/utils",
  "version": "1.0.0",
  "description": "Utils",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsc",
