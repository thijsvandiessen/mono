{
  "name": "@mono/search-site",
  "version": "1.0.0",
  "description": "search site",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./src/index.ts"
    }
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "lint": "npm run lint:js && npm run lint:styles && npm run lint:typescript",
    "lint:js": "eslint . --fix",
    "lint:styles": "prettier . --write  --list-different",
    "lint:typescript": "tsc",
    "test": "vitest run"
  },
  "dependencies": {
    "algoliasearch": "5.23.4",
    "dotenv": "16.5.0",
    "instantsearch.js": "4.78.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-instantsearch": "7.15.5"
  },
  "devDependencies": {
    "@mono/eslint-config": "1.0.0",
    "@mono/typescript-config": "1.0.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@types/node": "22.14.1",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "@vitejs/plugin-react": "4.3.4",
    "prettier": "3.5.3",
    "typescript": "5.8.3",
    "vite": "6.3.2",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.1.1"
  }
}
