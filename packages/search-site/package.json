{
  "name": "@mono/search-site",
  "version": "1.0.0",
  "description": "search site",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "pnpm run lint:js && pnpm run lint:styles",
    "lint:js": "eslint",
    "lint:styles": "prettier . --write  --list-different",
    "test": "vitest run -c ./vitest.config.ts"
  },
  "dependencies": {
    "algoliasearch": "5.46.2",
    "dotenv": "17.2.3",
    "instantsearch.js": "4.86.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-instantsearch": "7.22.1"
  },
  "devDependencies": {
    "@mono/eslint-config": "workspace:*",
    "@mono/typescript-config": "workspace:*",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.1",
    "@types/node": "25.0.6",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.2",
    "jsdom": "27.4.0",
    "prettier": "3.7.4",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "6.0.3",
    "vitest": "4.0.16"
  }
}
