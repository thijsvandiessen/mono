{
  "name": "@mono/graphql",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "./src/**/*",
  "scripts": {
    "test": "jest",
    "lint": "npm run lint:js && npm run lint:styles && npm run lint:typescript",
    "lint:js": "eslint . --fix",
    "lint:styles": "prettier . --write --cache --log-level silent --list-different",
    "lint:typescript": "tsc",
    "download-schema": "rover graph fetch vandiessen-mono@current > ./src/schema.graphql && npm run lint:styles",
    "codegen": "graphql-codegen --config ./codegen.ts",
    "codegen:watch": "graphql-codegen --require dotenv/config --config ./codegen.ts --watch"
  },
  "dependencies": {
    "react": "18.2.0"
  },
  "devDependencies": {
    "@apollo/rover": "0.21.0",
    "@commitlint/cli": "18.4.3",
    "@commitlint/config-conventional": "18.4.3",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "4.1.0",
    "@graphql-codegen/introspection": "4.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-operations": "4.0.1",
    "@testing-library/jest-dom": "6.1.5",
    "@testing-library/react": "14.1.2",
    "@types/jest": "29.5.11",
    "@types/node": "18.19.3",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "6.15.0",
    "@typescript-eslint/parser": "6.15.0",
    "dotenv": "16.3.1",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.4",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-testing-library": "6.2.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "pretty-quick": "3.1.3",
    "prettier": "3.1.1",
    "sass": "1.69.5",
    "turbo": "1.11.2",
    "typescript": "5.3.3",
    "webpack-bundle-analyzer": "4.10.1",
    "@graphql-codegen/typescript-document-nodes": "4.0.1"
  }
}
