{
  "name": "@mono/data",
  "version": "1.0.0",
  "description": "data",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsc",
    "codegen": "graphql-codegen-esm --config ./codegen.ts",
    "codegen:watch": "graphql-codegen-esm --require dotenv/config --config ./codegen.ts --watch",
    "dev": "tsc --watch",
    "download-schema": "rover graph fetch vandiessen-mono-graph@current > ./src/generated/schema.graphql && pnpm run lint:styles",
    "lint": "pnpm run lint:js && pnpm run lint:styles",
    "lint:js": "eslint",
    "lint:styles": "prettier . --write  --list-different",
    "test": "vitest run -c ./vitest.config.ts"
  },
  "dependencies": {
    "@graphql-typed-document-node/core": "3.2.0",
    "@mono/next-js": "workspace:*",
    "@mono/utils": "workspace:*",
    "@urql/core": "6.0.1",
    "@urql/exchange-graphcache": "8.1.0",
    "@urql/exchange-retry": "2.0.0",
    "graphql": "16.11.0",
    "react": "19.2.0"
  },
  "devDependencies": {
    "@apollo/rover": "0.36.2",
    "@commitlint/config-conventional": "20.0.0",
    "@graphql-codegen/cli": "6.0.1",
    "@graphql-codegen/client-preset": "5.1.1",
    "@graphql-codegen/introspection": "5.0.0",
    "@graphql-codegen/typescript": "5.0.2",
    "@graphql-codegen/typescript-document-nodes": "5.0.2",
    "@graphql-codegen/typescript-operations": "5.0.2",
    "@mono/eslint-config": "workspace:*",
    "@mono/typescript-config": "workspace:*",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@types/node": "24.9.2",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@vitejs/plugin-react": "5.1.0",
    "dotenv": "17.2.3",
    "eslint": "9.38.0",
    "eslint-plugin-testing-library": "7.13.3",
    "prettier": "3.6.2",
    "turbo": "2.5.8",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "4.0.4"
  }
}
