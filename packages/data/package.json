{
  "name": "@mono/data",
  "version": "1.0.0",
  "description": "data",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "vite build",
    "typecheck": "tsc",
    "codegen": "graphql-codegen-esm --config ./codegen.ts",
    "codegen:watch": "graphql-codegen-esm --require dotenv/config --config ./codegen.ts --watch",
    "dev": "vite build --watch",
    "download-schema": "rover graph fetch vandiessen-mono-graph@current > ./src/generated/schema.graphql && npm run lint:styles",
    "lint": "npm run lint:js && npm run lint:styles",
    "lint:js": "eslint . --fix",
    "lint:styles": "prettier . --write  --list-different",
    "test": "vitest run"
  },
  "dependencies": {
    "@mono/utils": "1.0.0",
    "@urql/core": "5.1.1",
    "@urql/exchange-graphcache": "7.2.3",
    "@urql/exchange-retry": "1.3.0",
    "graphql": "16.10.0",
    "react": "19.1.0"
  },
  "devDependencies": {
    "@apollo/rover": "0.28.1",
    "@commitlint/config-conventional": "19.8.0",
    "@graphql-codegen/cli": "5.0.5",
    "@graphql-codegen/client-preset": "4.8.0",
    "@graphql-codegen/introspection": "4.0.3",
    "@graphql-codegen/typescript": "4.1.6",
    "@graphql-codegen/typescript-document-nodes": "4.0.16",
    "@graphql-codegen/typescript-operations": "4.6.0",
    "@mono/eslint-config": "1.0.0",
    "@mono/typescript-config": "1.0.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@types/node": "22.14.1",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "@typescript-eslint/eslint-plugin": "8.30.1",
    "@typescript-eslint/parser": "8.30.1",
    "@vitejs/plugin-react": "4.3.4",
    "dotenv": "16.5.0",
    "eslint": "9.25.0",
    "eslint-plugin-testing-library": "7.1.1",
    "jest-environment-jsdom": "29.7.0",
    "prettier": "3.5.3",
    "turbo": "2.5.0",
    "typescript": "5.8.3",
    "vite-plugin-dts": "4.5.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.1.1"
  }
}
