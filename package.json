{
  "name": "mono-vandiessen",
  "version": "1.0.15",
  "private": true,
  "description": "Monorepo for small websites",
  "type": "module",
  "workspaces": [
    "packages/*",
    "apps/*",
    "e2e"
  ],
  "scripts": {
    "build": "tsc --build && pnpm run -r build",
    "codegen": "pnpm -r download-schema && pnpm -r codegen",
    "dev": "concurrently \"tsc --build --watch\" \"pnpm run --filter lundi-bleu dev\" \"pnpm run --filter example dev\"",
    "e2e": "pnpm run -r e2e",
    "lint": "pnpm run -r lint",
    "test": "vitest run -c ./vitest.config.ts"
  },
  "devDependencies": {
    "@apollo/rover": "0.36.2",
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@swc/core": "1.15.2",
    "@types/node": "24.10.1",
    "concurrently": "9.2.1",
    "dotenv": "17.2.3",
    "eslint": "9.39.1",
    "prettier": "3.6.2",
    "ts-node": "10.9.2",
    "turbo": "2.6.1",
    "typescript": "5.9.3",
    "vitest": "4.0.10"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.53.2"
  },
  "packageManager": "pnpm@10.21.0+sha512.da3337267e400fdd3d479a6c68079ac6db01d8ca4f67572083e722775a796788a7a9956613749e000fac20d424b594f7a791a5f4e2e13581c5ef947f26968a40"
}
