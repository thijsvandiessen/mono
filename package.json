{
  "name": "mono-vandiessen",
  "version": "1.0.15",
  "private": true,
  "description": "Monorepo for small websites",
  "type": "module",
  "workspaces": [
    "packages/*",
    "apps/*",
    "e2e"
  ],
  "scripts": {
    "build": "tsc --build && pnpm run -r build",
    "codegen": "pnpm -r download-schema && pnpm -r codegen",
    "dev": "concurrently \"tsc --build --watch\" \"pnpm run --filter lundi-bleu dev\" \"pnpm run --filter example dev\"",
    "e2e": "pnpm run -r e2e",
    "lint": "pnpm run -r lint",
    "test": "vitest run -c ./vitest.config.ts"
  },
  "devDependencies": {
    "@apollo/rover": "0.36.2",
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@swc/core": "1.14.0",
    "@types/node": "24.9.2",
    "concurrently": "9.2.1",
    "dotenv": "17.2.3",
    "eslint": "9.39.0",
    "prettier": "3.6.2",
    "ts-node": "10.9.2",
    "turbo": "2.6.0",
    "typescript": "5.9.3",
    "vitest": "4.0.6"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.52.5"
  },
  "packageManager": "pnpm@10.19.0+sha512.c9fc7236e92adf5c8af42fd5bf1612df99c2ceb62f27047032f4720b33f8eacdde311865e91c411f2774f618d82f320808ecb51718bfa82c060c4ba7c76a32b8"
}
